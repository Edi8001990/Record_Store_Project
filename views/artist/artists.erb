<h2 class="header">Artists Overview</h2>


<% @artists.each do |artist|  %>
<div class="artists-list-block">
<table>
    <tr>
      <th colspan="2" class="artists-links"><a href="/artists/<%= artist.id %>"><%= artist.name %></a></th>
    </tr>

<% artist.albums.each do |album| %>
<tr>
  <td><%= album.album_title %></td>
  <td class="artists-links"><a href="/albums/<%= album.id %>">Album Info</a></td>
</tr>


<% end %>


</table>
</div>
<% end %>
